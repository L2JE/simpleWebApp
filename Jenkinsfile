  agent any
  
  stages{
    stage ("fst_stage") {
      steps {
        //do something
        echo 'DOING FIRST STEP ON FIRST STAGE'
      
      }
    }
    stage ("snd_stage") {
      steps {
        //do something
        echo 'ENTERING SECOND STAGE'
      }
    }
    stage ("trd_stage") {
      steps {
        echo 'HERE ON THIRD STAGE'
      
      }
    }
  }
}
